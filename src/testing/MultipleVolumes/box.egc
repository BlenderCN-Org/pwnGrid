<engridcase>
  <engrid>
    <bc>
      1 BC1 patch
      2 BC2 patch
      3 BC3 patch
      4 BC4 patch
      -1 box 1:1,2:1,3:1,4:0
      -2 cyl 1:0,2:-1,3:0,4:-1
    </bc>
    <physical>
    </physical>
  </engrid>
  <openfoam>
    <simplefoam>
      <standard>
        <fvschemes>
 version int = 7;
 version double = 42;
 ddtSchemes int  = 42;
 ddtSchemes double  = 3.14;
 ddtSchemes text  = simpleFoam;
 U-convection-scheme = Gauss upwind;
 k-convection-scheme = Gauss linearUpwind Gauss;
 omega-convection-scheme = Gauss limitedLinearV 1;
 fluxRequired 1 =  yes;
 fluxRequired 2 =  no;
</fvschemes>
        <fvschemes2>
 version int = 7;
 version double = 42;
 ddtSchemes int  = 42;
 ddtSchemes double  = 3.14;
 ddtSchemes text  = some other simpleFoam file;
 U-convection-scheme = Gauss upwind;
 k-convection-scheme = Gauss linearUpwind Gauss;
 omega-convection-scheme = Gauss limitedLinearV 1;
 fluxRequired 1 =  yes;
 fluxRequired 2 =  no;
</fvschemes2>
      </standard>
    </simplefoam>
    <rhosimplefoam>
      <standard>
        <fvschemes>
 version int = 7;
 version double = 42;
 ddtSchemes int  = 42;
 ddtSchemes double  = 3.14;
 ddtSchemes text  = rhoSimpleFoam;
 U-convection-scheme = Gauss upwind;
 k-convection-scheme = Gauss linearUpwind Gauss;
 omega-convection-scheme = Gauss limitedLinearV 1;
 fluxRequired 1 =  yes;
 fluxRequired 2 =  no;
</fvschemes>
        <fvschemes2>
 version int = 7;
 version double = 42;
 ddtSchemes int  = 42;
 ddtSchemes double  = 3.14;
 ddtSchemes text  = some other rhoSimpleFoam file;
 U-convection-scheme = Gauss upwind;
 k-convection-scheme = Gauss linearUpwind Gauss;
 omega-convection-scheme = Gauss limitedLinearV 1;
 fluxRequired 1 =  yes;
 fluxRequired 2 =  no;
</fvschemes2>
      </standard>
    </rhosimplefoam>
  </openfoam>
</engridcase>

