<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="QT Help Collection File Generation"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="QCollectionGenerator"
			DisplayName="QT Help Collection File Generation"
			CommandLine="$(SolutionDir)\qtvars.bat $(PlatformName) qcollectiongenerator.exe &quot;$(InputDir)$(InputFileName)&quot; -o &quot;$(InputDir)$(InputName).qhc&quot;&#x0D;&#x0A;"
			Outputs="&quot;$(InputDir)\$(InputName).qhc&quot;"
			FileExtensions="*.qhcp"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
